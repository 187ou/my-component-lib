{
  "name": "my-component-lib",
  "version": "0.1.0",
  "type": "module",
  "private": false,
  "description": "A Vue 3 component library",
  "keywords": [
    "vue3",
    "component",
    "ui"
  ],
  "author": "Your Name <your@email.com>",
  "license": "MIT",
  "homepage": "https://github.com/yourname/my-component-lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/my-component-lib.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/my-component-lib.umd.js",
  "module": "./dist/my-component-lib.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/my-component-lib.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/my-component-lib.umd.js"
      }
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .vue,.js,.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "dependencies": {
    "vue": "^3.2.0 || ^3.3.0 || ^3.4.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0 || ^3.3.0 || ^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^27.2.0",
    "sass": "^1.94.2",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-inspect": "^0.8.9",
    "vitepress": "^1.0.0-rc.44",
    "vitest": "^1.6.1",
    "vue-tsc": "^2.0.6"
  }
}
